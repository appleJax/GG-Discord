{
  "name": "gamegogakuen-discord-bot",
  "version": "1.0.0",
  "description": "GameGogakuen's Discord Quiz Bot",
  "main": "index.js",
  "scripts": {
    "buildit": "webpack --config webpack.prod.js",
    "serve:staging": "webpack && node ./dist/main.js",
    "serve:prod": "webpack --config webpack.prod.js && ADMIN=true node ./dist/main.js",
    "shipit": "git checkout -b deploy && npm run buildit && rm .gitignore && echo 'node_modules\n.env.js' > .gitignore && rm -rf uploads && git add . && git commit -m 'build' && git push -f heroku deploy:master && git checkout master && git branch -D deploy",
    "start": "node ./dist/main.js",
    "test": "jest --runInBand"
  },
  "keywords": [],
  "author": "",
  "engines": {
    "node": "^10.16.0"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "bufferutil": "^3.0.3",
    "cloudinary": "^1.11.0",
    "core-js": "^3.6.1",
    "discord.js": "^11.5.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-authorization": "^1.0.0",
    "express-session": "^1.15.6",
    "mongoose": "^5.8.4",
    "multer": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.3",
    "regenerator-runtime": "^0.13.3",
    "unescape": "^1.0.1",
    "unzip-stream": "^0.3.0",
    "urlencode": "^1.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "jest-cli": "^23.6.0",
    "webpack": "^4.8.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "automock": false,
    "testPathIgnorePatterns": [
      "node_modules",
      ".*\\.json\\.js"
    ],
    "moduleNameMapper": {
      "^Anki(.*)$": "<rootDir>/src/anki$1",
      "^Bot(.*)$": "<rootDir>/src/bot$1",
      "^Config(.*)$": "<rootDir>/src/config$1",
      "^DB(.*)$": "<rootDir>/src/db$1",
      "^Models(.*)$": "<rootDir>/src/db/models$1",
      "^Src(.*)$": "<rootDir>/src$1",
      "^Utils$": "<rootDir>/src/utils.js"
    }
  }
}
